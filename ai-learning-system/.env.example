# AI API 配置
# 支持 OpenAI 兼容的 API (如心流, DeepSeek, 硅基流动等)
AI_API_KEY=your_api_key_here
AI_API_BASE=https://apis.iflow.cn/v1
AI_MODEL=TBStars2-200B-A13B

# 备用配置示例 (心流)
# AI_API_KEY=your_iflow_key
# AI_API_BASE=https://apis.iflow.cn/v1
# AI_MODEL=TBStars2-200B-A13B

# 备用配置示例 (硅基流动 DeepSeek)
# AI_API_KEY=your_siliconflow_key
# AI_API_BASE=https://api.siliconflow.cn/v1
# AI_MODEL=deepseek-ai/DeepSeek-R1

# OAuth 认证配置（启用后支持 API Key 自动续期）
OAUTH_ENABLED=false
OAUTH_CLIENT_ID=your_client_id_here
OAUTH_CLIENT_SECRET=your_client_secret_here
OAUTH_AUTH_URL=https://auth.iflow.cn/oauth/authorize
OAUTH_TOKEN_URL=https://auth.iflow.cn/oauth/token
OAUTH_REDIRECT_URI=http://localhost:6066/api/auth/callback
OAUTH_SCOPE=read write

# 服务端口
APP_PORT=6066

# MySQL 数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_password_here
MYSQL_DATABASE=learnflow
